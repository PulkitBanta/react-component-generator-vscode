{
    "name": "react-component-generator",
    "displayName": "React Component Generator",
    "description": "Generate react component with sample code in vscode",
    "publisher": "pulkitbanta",
    "version": "1.0.0",
    "author": {
        "name": "pulkit banta"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "homepage": "https://github.com/PulkitBanta/react-component-generator-vscode/blob/main/README.md",
    "bugs": {
        "url": "https://github.com/PulkitBanta/react-component-generator-vscode/issues",
        "email": "pulkitbanta01@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PulkitBanta/react-component-generator-vscode"
    },
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Snippets",
        "Programming Languages"
    ],
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#3e3e3e",
        "theme": "dark"
    },
    "activationEvents": [
        "onCommand:react-component-generator.create-react-component"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "react-component-generator.create-react-component",
                "title": "Create React Component"
            }
        ]
    },
    "prettier": "@medly/prettier-config",
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@commitlint/cli": "^14.1.0",
        "@commitlint/config-conventional": "^14.1.0",
        "@medly/prettier-config": "^0.1.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.61.0",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "@vscode/test-electron": "^1.6.2",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "glob": "^7.1.7",
        "husky": "^7.0.4",
        "mocha": "^9.1.1",
        "typescript": "^4.4.3"
    }
}
